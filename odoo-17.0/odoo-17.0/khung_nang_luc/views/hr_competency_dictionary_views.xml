<odoo>
    <record id="view_hr_competency_dictionary_tree" model="ir.ui.view">
        <field name="name">hr.competency.dictionary.tree</field>
        <field name="model">hr.competency.dictionary</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="level_id"/>
            </tree>
        </field>
    </record>

    <record id="view_hr_competency_dictionary_form" model="ir.ui.view">
        <field name="name">hr.competency.dictionary.form</field>
        <field name="model">hr.competency.dictionary</field>
        <field name="arch" type="xml">
            <form string="Từ điển năng lực">
                <group>
                    <field name="name"/>
                    <field name="description"/>
                    <field name="level_id"/>
                </group>
            </form>
        </field>
    </record>

    <menuitem id="menu_hr_competency_dictionary" name="Từ điển năng lực" parent="menu_khung_nang_luc_cauhinh" action="action_hr_competency_dictionary"/>

    <record id="action_hr_competency_dictionary" model="ir.actions.act_window">
        <field name="name">Từ điển năng lực</field>
        <field name="res_model">hr.competency.dictionary</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
